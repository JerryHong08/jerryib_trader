# Data Manager Selection
# Choose one: polygon, theta, simulator
DATA_MANAGER=polygon

# Polygon.io Configuration
# Get your API key from: https://polygon.io/dashboard/api-keys
POLYGON_API_KEY=your_polygon_api_key_here

# ThetaData Configuration
# Make sure ThetaData Terminal is running on localhost:25520
# No additional configuration needed

# Simulator Configuration
# Format: YYYYMMDD
REPLAY_DATE=20251206
# Optional: Start time for replay in HH:MM or HH:MM:SS format (America/New_York timezone)
REPLAY_START_TIME=09:30

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8000
